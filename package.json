{
  "name": "nestjs-starter",
  "version": "0.0.0",
  "description": "Node.js Nest framework starter",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "del-cli dist && nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "jest --detectOpenHandles",
    "test:e2e": "jest --config ./test/jest.e2e.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.3.2",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "^7.3.2",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/typeorm": "^7.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@catsmiaow/eslint-config": "^0.6.0",
    "@catsmiaow/tsconfig": "^0.2.1",
    "@nestjs/cli": "^7.4.1",
    "@nestjs/testing": "^7.3.2",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@types/supertest": "^2.0.10",
    "del-cli": "^3.0.1",
    "dtslint": "^3.6.12",
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  }
}
